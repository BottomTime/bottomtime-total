{
  "name": "@bottomtime/service",
  "version": "1.0.0",
  "description": "BottomTime backend service",
  "main": "src/index.ts",
  "author": "Chris Carleton <mrchriscarleton@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "admin": "tsx admin/bt-admin.ts",
    "build": "webpack",
    "clean": "rimraf dist/",
    "dev": "webpack --watch",
    "docs": "swagger-jsdoc -d swaggerDefinition.cjs -o public/docs/swagger.json ./src/**/*.ts",
    "migrate:create": "typeorm-ts-node-commonjs migration:create",
    "migrate:down": "typeorm-ts-node-commonjs migration:revert -d data-source.ts",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate -d data-source.ts",
    "migrate:up": "typeorm-ts-node-commonjs migration:run -d data-source.ts",
    "prepare": "cp -n .env.example .env || true",
    "serve": "pm2 start --no-daemon dist/main.js",
    "test": "jest"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.515.0",
    "@aws-sdk/s3-request-presigner": "^3.515.0",
    "@bottomtime/api": "workspace:*",
    "@bottomtime/common": "workspace:*",
    "@bottomtime/templates": "workspace:*",
    "@codegenie/serverless-express": "^4.14.1",
    "@nestjs/common": "^10.2.10",
    "@nestjs/core": "^10.2.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.10",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/typeorm": "^10.0.2",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "cookie-parser": "^1.4.6",
    "crypto-browserify": "^3.12.0",
    "dayjs": "^1.11.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "helmet": "^7.1.0",
    "jimp": "^0.22.12",
    "jsonwebtoken": "^9.0.1",
    "mustache": "^4.2.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-anonymous": "^1.0.1",
    "passport-discord": "^0.1.4",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "prompts": "^2.4.2",
    "reflect-metadata": "^0.1.13",
    "request-stats": "^3.0.0",
    "rxjs": "^7.8.1",
    "serverless": "^4.1.0",
    "ts-loader": "^9.5.1",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.0",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@types/aws-lambda": "^8.10.140",
    "@types/bcryptjs": "^2.4.6",
    "@types/bunyan": "^1.8.8",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-useragent": "^1.0.2",
    "@types/jest": "^29.4.0",
    "@types/multer": "^1.4.11",
    "@types/mustache": "^4.2.5",
